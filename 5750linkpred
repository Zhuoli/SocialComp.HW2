#!/usr/bin/env python

import sys, random

if len(sys.argv) < 3:
    print 'Usage:', sys.argv[0], '<graph file> <# of edges to predict>'
    sys.exit()

try:
    graphfile = open(sys.argv[1])
except:
    print 'Error: Unable to open graph file.'
    sys.exit()

try:
    num_edges = int(sys.argv[2])
except:
    print 'Error: Unable to parse # of edges from command line'
    sys.exit()

nodes = set()
for line in graphfile:
    try:
        n1, n2 = line.split()
    except:
        print 'Error: Malformed graph file.'
        sys.exit()

    nodes.add(n1)
    nodes.add(n2)

for i in range(num_edges):
    n1, n2 = random.sample(nodes, 2)
    print '%s\t%s' % (n1, n2)
